{
  "name": "@fastgas/server",
  "version": "0.0.1",
  "private": true,
  "description": "The back-end of the FastGas inventory app",
  "license": "MIT",
  "author": "Vitor Gouveia <vitor.gouveia@aluno.etecjrm.com.br>",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "deploy": "npm run build && npm run start",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --exit-child --no-notify src/index.ts",
    "push": "prisma db push --accept-data-loss",
    "start": "node dist/server.js",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^2.30.3",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.7.10",
    "@types/uuid": "^8.3.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^26.0.1",
    "prisma": "^2.30.3",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "uuid": "^8.3.2"
  }
}
